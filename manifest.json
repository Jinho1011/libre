{
    "manifest_version": 2,
    "name": "Libre :: free your site",
    "version": "0.1.0",
    "description": "Modify CSS and HTML",

    "icons": {
        "24": "icon.png"
    },

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "css": ["style.css"],
        "matches": ["https://www.benedu.co.kr/Index_m.aspx"]
    }],

    "permissions": [
        "activeTab",
        "http://*/",
        "storage"
      ]
}